        inherit! :search_paths
        # Pods for testing
        pod 'OHHTTPStubs'
        pod 'OHHTTPStubs/Swift'
    end
    
    target 'BabylonHealthUITests' do
        inherit! :search_paths
        # Pods for testing
    end
    
end
